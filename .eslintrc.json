{
  "extends": ["airbnb", "plugin:fsd/all"],
  "plugins": ["fsd"],
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "global-require": "off",
    "react/sort-comp": "off",
    "max-len": "off",
    "no-new": "off",
    "no-underscore-dangle": "off"
  },
  "env": {
    "browser": true,
    "jquery": true
  }
}
